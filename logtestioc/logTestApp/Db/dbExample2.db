record(calc, "$(prefix):calcExample$(no)")
{
	alias("$(prefix):calc$(no)")
	field(DESC, "Counter No. $(no)")
	field(SCAN,"$(scan)")
	field(FLNK, "$(prefix):aiExample$(no)")
	field(CALC, "(A<B)?(A+C):D")
	field(INPA, "$(prefix):calcExample$(no).VAL NPP NMS")
	field(INPB, "9")
	field(INPC, "1")
	field(INPD, "0")
	field(EGU, "Counts")
	field(HOPR, "10")
	field(HIHI, "8")
	field(HIGH, "6")
	field(LOW, "4")
	field(LOLO, "2")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
	field(LSV, "MINOR")
	field(LLSV, "MAJOR")
}
record(ai, "$(prefix):aiExample$(no)")
{
	field(DESC, "Analog input No. $(no)")
	field(INP, "$(prefix):calcExample$(no).VAL NPP NMS")
	field(EGUF, "10")
	field(EGU, "Counts")
	field(HOPR, "10")
	field(LOPR, "0")
	field(HIHI, "8")
	field(HIGH, "6")
	field(LOW, "4")
	field(LOLO, "2")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
	field(LSV, "MINOR")
	field(LLSV, "MAJOR")
}
alias("$(prefix):aiExample$(no)","$(user):ai$(no)")
